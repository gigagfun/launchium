# Next.js Environment Variables for Vercel Deployment

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://launchium.io
NEXT_PUBLIC_APP_NAME=Launchium

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=your-google-analytics-id

# Social Media (Optional)
NEXT_PUBLIC_TWITTER_HANDLE=@launchium
NEXT_PUBLIC_DISCORD_INVITE=your-discord-invite-link

# Email Configuration (Optional)
NEXT_PUBLIC_CONTACT_EMAIL=hello@launchium.io

# Token Information
NEXT_PUBLIC_TOKEN_SYMBOL=LNCHM
NEXT_PUBLIC_TOKEN_NAME=Launchium
NEXT_PUBLIC_CONTRACT_ADDRESS=0x...

# Presale Configuration
NEXT_PUBLIC_PRESALE_START_DATE=2024-07-04T00:00:00
NEXT_PUBLIC_PRESALE_END_DATE=2024-07-09T23:59:59
NEXT_PUBLIC_HARD_CAP=1750000
NEXT_PUBLIC_TOKEN_PRICE=0.005 